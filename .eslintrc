{
  "env": {
    "browser": true,
    "node": true,
    "es6": true
  },
  "extends": [
    "eslint:recommended",
    "plugin:prettier/recommended"
  ],
  "parserOptions": {
    "ecmaVersion": "latest",
    "sourceType": "module"
  },
  "plugins": [
    "simple-import-sort"
  ],
  "rules": {
    "no-underscore-dangle": "off",
    "simple-import-sort/exports": "warn",
    "simple-import-sort/imports": "warn"
  }
}
